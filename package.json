{
  "name": "antigravity-proxy",
  "version": "1.0.0",
  "description": "Node.js proxy to intercept Google Antigravity IDE API calls and use your own Gemini API token",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node src/test.js",
    "launch": "./scripts/launch-antigravity.sh",
    "logs": "tail -f logs/proxy.log"
  },
  "keywords": [
    "proxy",
    "gemini",
    "antigravity",
    "google",
    "ai",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "http-proxy": "^1.18.1",
    "dotenv": "^16.4.5",
    "winston": "^3.11.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
